{
  "name": "No Fake News",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Mark fake news on your Facebook feed",
  "browser_action": {
    "name": "No Fake News",
    "icons": ["icon.png"],
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
    "js": [ "jquery.js", "content.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
